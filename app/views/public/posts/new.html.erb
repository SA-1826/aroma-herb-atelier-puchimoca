<div class="d-flex justify-content-center">
  <%= render 'public/searches/search_form' %>
</div>
<h1>新規投稿</h1>

<%= form_with model: @post do |f| %>
  <%= render 'layouts/error_messages', model: f.object %>

  <h4>タイトル</h4>
  <%= f.text_field :title %>
	
  <h4>本文</h4>
  <%= f.text_area :body %>
	
  <%= f.submit '投稿' %>
<% end %>

