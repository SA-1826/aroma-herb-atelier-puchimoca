<h1>講座-一覧</h1>

  <table class="table table-hover">
    <thead>
      <tr>
        <th>ステータス</th>
        <th>タイトル</th>
        <th>開催日時</th>
        <th>作成日時</th>
      </tr>
    </thead>
    <tbody>
      <% @programs.each do |program| %>
        <tr>
          <td>
            <%= program.status %>
          </td>
          <td>
            <%= link_to edit_admin_program_path(program) do %>
              <%= program.title %>
            <% end %>
          </td>
          <td>
            <%= post.start_time %>
          </td>
          <td>
            <%= post.created_at %>
          </td>
        </tr>
      <% end %>
    </tbody>
  </table>

